#!/usr/bin/env node

/**
 * Module dependencies.
 */

var app = require('../app');
var debug = require('debug')('voting-app:server');
var http = require('http');

app.listen(process.env.PORT || 3000, process.env.IP, function(err){
  if(err){
    console.log(err);
  } else {
    console.log('Voting App Server is Up.')
  }
})
